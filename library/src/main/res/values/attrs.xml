<?xml version="1.0" encoding="utf-8"?>
<resources>

  <declare-styleable name="DiagonalImageView">
    <attr name="di_start" format="enum">
      <enum name="left" value="1"/>
      <enum name="top" value="2"/>
      <enum name="right" value="3"/>
      <enum name="bottom" value="4"/>
    </attr>
    <attr name="di_end" format="enum">
      <enum name="left" value="1"/>
      <enum name="top" value="2"/>
      <enum name="right" value="3"/>
      <enum name="bottom" value="4"/>
    </attr>
    <attr name="di_distance" format="dimension"/>
    <attr name="di_borderSize" format="dimension"/>
    <attr name="di_borderColor" format="color"/>
    <attr name="di_borderEnabled" format="boolean"/>
  </declare-styleable>

</resources>